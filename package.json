{
  "name": "typescript-template",
  "version": "1.0.0",
  "description": "A typescript with nodejs (and mongoDB) template for quick start on projects",
  "main": "server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "prestart": "tsc",
    "build": "tsc",
    "test": "mocha -r ts-node/register mocha-lcov-reporter > coverage/coverage.lcov  src/**/*.spec.ts --reporter nyan",
    "dev": "ts-node ./src/server.ts",
    "test-dev": "mocha -r ts-node/register src/**/*.spec.ts",
    "lint": "node_modules/.bin/goodparts src/**/*.ts"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Gabriel Bolzi de Souza",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/body-parser": "1.17.0",
    "@types/chai": "4.1.7",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "@types/helmet": "0.0.42",
    "@types/mocha": "5.2.6",
    "@types/mongoose": "5.3.8",
    "@types/morgan": "1.7.35",
    "@types/node": "10.12.18",
    "@types/supertest": "2.0.7",
    "chai": "4.2.0",
    "goodparts": "1.2.1",
    "mocha": "6.1.3",
    "supertest": "4.0.2",
    "ts-node": "8.0.1",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1"
  },
  "dependencies": {
    "axios": ">=0.18.1",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-healthcheck": "0.1.0",
    "helmet": "3.15.0",
    "mongoose": "5.4.3",
    "morgan": "1.9.1",
    "typescript": "3.2.4"
  }
}
